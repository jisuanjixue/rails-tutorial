<% provide(:title, 'Coordinator Register') %>
<div class="col-md-6 col-md-offset-4"><h1>Coordinator Register</h1>
<hr style="FILTER: alpha(opacity=100,finishopacity=0,style=3)" width="80%" color=#987cb9 SIZE=3>
</div>
<div class="row">
  <div class="jumbotron bg-1 col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= render 'common/error_messages' %>
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
          <%= f.label :email %>
          <%= f.email_field :email, class: 'form-control' %>
          <%= f.label :password %>
          <%= f.password_field :password, class: 'form-control' %>
          <%= f.label :image %>
          <%= f.file_field :image, as: :file %>
      <span class="col-md-8 col-md-offset-4"><%= f.submit "Register", class: "btn btn-primary" %></span>
    <% end %>
  </div>
</div>
